{
  "prettier.documentSelectors": ["**/*.svelte","**/*.astro"]
  "eslint.probe": ["javascript", "typescript", "svelte", "astro"],
    "eslint.validate": [
    "javascript",
    "typescript", 
    "svelte", 
    "astro", 
  ],
  "[svelte]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[astro]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.words": ["trpc"]
}
